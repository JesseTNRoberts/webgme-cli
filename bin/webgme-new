#!/usr/bin/env node

'use strict';
var Command = require('commander').Command,
    program = new Command();

require('../src/BinUtils')
    .getSupportedSubCommands(__dirname)
    .forEach(function(component) {
        program
            .command(component.cmd + ' <name>', 'Create a new ' + component.name);
    });

program.parse(process.argv);
